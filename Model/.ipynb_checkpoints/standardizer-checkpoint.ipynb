   "source": [
    "def standardize(X):\n",
    "    f = open('mean.pckl', 'rb')\n",
    "    meanX = pickle.load(f)\n",
    "    f.close()\n",
    "    \n",
    "    f = open('std.pckl', 'rb')\n",
    "    stdX = pickle.load(f)\n",
    "    f.close()\n",
    "    \n",
    "    return (X - meanX) / stdX"
   ]
  }
